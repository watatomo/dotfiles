{
    "editor.largeFileOptimizations": false,
    "git.enableSmartCommit": true,
    "git.postCommitCommand": "push",
    "explorer.confirmDragAndDrop": false,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorTheme": "Catppuccin Mocha",
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "none",
    "git.confirmSync": false,
    "stylelint.customSyntax": "postcss-scss",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontFamily": "'Victor Mono', 'FiraCode NF', monospace",
    "editor.fontLigatures": true,
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "editor.wordWrap": "on",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "files.exclude": {
        ".next/": true,
        "**/package-lock.json": true,
        "node_modules/": true
    },
    "prettier.tabWidth": 4,
    "git.autofetch": true,
    "editor.bracketPairColorization.enabled": true,
    "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
    "workbench.preferredLightColorTheme": "Catppuccin Latte",
    "workbench.colorCustomizations": {
        "[Catppuccin Mocha]": {
            "tab.border": "#1e1e2e",
            "tab.inactiveBackground": "#1e1e2e",
            "tab.activeBorder": "#cdd6f4",
            "tab.unfocusedHoverForeground": "#7f849c",
            "statusBar.foreground": "#9399b2",
            "menu.separatorBackground": "#585b70",
            "editor.foldBackground": "#89b4fa25",
            "sideBar.foreground": "#bac2de",
            "sideBarTitle.foreground": "#bac2de",
            "sideBarSectionHeader.foreground": "#bac2de",
            "list.activeSelectionForeground": "#bac2de",
            "list.focusForeground": "#bac2de",
            "list.inactiveSelectionForeground": "#bac2de",
            "list.hoverBackground": "#313244",
            "list.inactiveSelectionBackground": "#313244",
            "list.deemphasizedForeground": "#bac2de",
            "editorOverviewRuler.background": "#1e1e2e",
            "breadcrumb.foreground": "#9399b2",
            "symbolIcon.arrayForeground": "#9399b2",
            "symbolIcon.booleanForeground": "#9399b2",
            "symbolIcon.classForeground": "#9399b2",
            "symbolIcon.constantForeground": "#9399b2",
            "symbolIcon.moduleForeground": "#9399b2"
        }
    },
    "rpc.lowerDetailsEditing": "Working on {file_name}",
    "rpc.problemsText": "",
    "editor.minimap.enabled": false,
    "telemetry.telemetryLevel": "off",
    "languageToolLinter.languageTool.motherTongue": "en-US",
    "languageToolLinter.lintOnSave": false,
    "languageToolLinter.serviceType": "public",
    "grammarly.config.documentDomain": "creative",
    "grammarly.config.documentDialect": "american",
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "**/*.md"
    ],
    "grammarly.config.suggestions.OxfordComma": true,
    "grammarly.config.suggestions.MissingSpaces": false,
    "editor.codeActionsOnSave": {},
    "editor.fontWeight": "500",
    "debug.console.fontFamily": "editor",
    "terminal.integrated.fontWeight": "500",
    "editor.formatOnType": true,
    "markdown.preview.lineHeight": 1.5,
    "editor.cursorBlinking": "phase",
    "workbench.iconTheme": "file-icons",
    "colorize.colorized_colors": [
        // "BROWSERS_COLORS",
        "HEXA",
        "RGB",
        "HSL"
    ],
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "eslint.alwaysShowStatus": true,
    "eslint.codeActionsOnSave.rules": null,
    "stylelint.config": {
        "rules": {
            "indentation": 4,
            "number-leading-zero": "always",
            "max-empty-lines": 1,
            "string-quotes": "double",
            "selector-max-id": 0,
            "selector-list-comma-newline-after": "always",
            "rule-empty-line-before": [
                "always",
                {
                    "ignore": ["after-comment", "first-nested"]
                }
            ],
            "comment-empty-line-before": [
                "always",
                {
                    "except": ["first-nested"]
                }
            ],
            "block-opening-brace-space-before": "always",
            "declaration-colon-space-after": "always",
            "declaration-colon-space-before": "never",
            "declaration-block-single-line-max-declarations": 1,
            "declaration-property-value-blacklist": {
                "/^border/": ["none"]
            },
            "at-rule-empty-line-before": [
                "always",
                {
                    "ignore": ["after-comment"],
                    "except": ["after-same-name", "first-nested"]
                }
            ],
            "scss/dollar-variable-pattern": "^_?[a-z]+[\\w-]*$",
            "scss/at-extend-no-missing-placeholder": true,
            "order/order": [
                "declarations",
                {
                    "type": "at-rule"
                },
                {
                    "type": "at-rule",
                    "hasBlock": true
                },
                "rules"
            ],
            "shorthand-property-no-redundant-values": true
        }
    },
    "editor.linkedEditing": true,
    "colorize.languages": [
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg",
        "toml"
    ],
    "[lua]": {
        "editor.defaultFormatter": "sumneko.lua"
    },
    "Lua.telemetry.enable": false,
    "Lua.format.defaultConfig": {
        "indent_style": "space",
        "indent_size": "4",
        "end_of_line": "lf",
        "quote_style": "double"
    },
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.external.windowsExec": "C:\\Program Files\\WezTerm\\wezterm-gui.exe",
    "terminal.integrated.env.windows": {
        "PATH": "C:\\Program Files\\starship\\bin;${env:PATH}"
    },
    "terminal.explorerKind": "external",
    "prettier.trailingComma": "none",
    "editor.semanticTokenColorCustomizations": {}
}
